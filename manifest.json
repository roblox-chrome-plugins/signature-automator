{
	"name": "Siggu Automator",
	"version": "1",
	"description": "Made by mustyoshi. A signiture automator for the forums.",
	"browser_action": {
		"default_icon": "icon.png",
		"popup": "popup.html"
	},
	"background_page": "back.html",
	"content_scripts": [
		{
			"matches": ["http://*.roblox.com/Forum/AddPost.aspx?ForumID=*","http://*.roblox.com/Forum/ShowPost.aspx?PostID=*"],
			"js": ["post.js"]

		}
	],
	"icons": {
		"128": "icon.png"
	},
	"permissions": ["http://*.roblox.com/*", "https://*.roblox.com/*"]
}